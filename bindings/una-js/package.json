{
  "name": "@blc-org/una",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "una",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.11.4",
    "ava": "^4.3.3",
    "json-schema-to-typescript": "~11.0.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version",
    "postbuild": "yarn generate-types",
    "generate-types": "cargo run --manifest-path ../../schemas/Cargo.toml schemas && node generate-types.js"
  },
  "packageManager": "yarn@3.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/blc-org/una",
    "directory": "bindings/una-js"
  },
  "description": "Manage any Lightning node with UNA, the Universal (Lightning) Node API",
  "ava": {
    "timeout": "2m",
    "workerThreads": false
  }
}
